<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uuabb.miao.dao.UserDao">
    <cache type="com.uuabb.miao.utils.RedisCache"></cache>

    <select id="getCount" resultType="java.lang.Integer">
        SELECT COUNT(1) FROM user
    </select>
    <insert id="insertUser" useGeneratedKeys="true" keyColumn="area_id" keyProperty="areaId"
            parameterType="com.uuabb.miao.entity.User">
      INSERT INTO user
      ( user_name,user_password,user_email,role,permission,deviceId)
      VALUE
      (#{userName},#{userPassword},#{userEmail},#{role},#{permission},#{deviceId})
    </insert>

</mapper>

